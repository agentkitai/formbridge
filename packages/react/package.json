{
  "name": "@formbridge/react",
  "version": "0.0.0",
  "description": "React hooks and components for FormBridge",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "forms",
    "react",
    "hooks",
    "typescript"
  ],
  "author": "FormBridge Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/formbridge/formbridge.git",
    "directory": "packages/react"
  },
  "dependencies": {
    "@formbridge/core": "*"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0"
  }
}
