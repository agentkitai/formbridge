# FormBridge Environment Variables
# Copy this file to .env and fill in your values.

# Webhook signing secret for HMAC-SHA256 signatures.
# Required for secure webhook delivery. Generate with: openssl rand -hex 32
FORMBRIDGE_WEBHOOK_SECRET=

# Server port (default: 3000)
FORMBRIDGE_PORT=3000

# Base URL for generating resume/handoff URLs
FORMBRIDGE_BASE_URL=http://localhost:3000
